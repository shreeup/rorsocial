<div class="container">
<p style="color: green"><%= notice %></p>

<h1>Items</h1>

<div id="items">
  <% @items.each do |item| %>
    <%= render item %>
    <p>
      <%= link_to "Show this item", item %>
    </p>
  <% end %>
</div>
<% if user_signed_in? %>
<%= link_to "New item", new_item_path %>
<% end %>
</div>