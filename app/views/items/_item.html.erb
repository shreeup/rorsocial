<div id="<%= dom_id item %>">
  <p>
    <strong>Votes:</strong>
    <%= item.votes_for.up.size %>
  </p>

  <p>
    <strong>User:</strong>
    <%= item.user_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= item.title %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= item.url %>
  </p>

  <p>
    <strong>Text:</strong>
    <%= item.text %>
  </p>

</div>
